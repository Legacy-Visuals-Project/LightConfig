{
	"schemaVersion": 1,
	"id": "${id}",
	"version": "${version}",
	"name": "${name}",
	"description": "${description}",
	"authors": [
		"lowercasebtw",
		"mixces"
	],
	"contact": {
		"sources": "${source}",
		"issues": "${issues}"
	},
	"license": "${license}",
	"icon": "assets/${id}/icon.png",
	"environment": "client",
	"depends": {
		"fabricloader": ">=${fabric_loader_version}",
		"minecraft": "${minecraft_version_range}",
		"fabric-api": "*"
	},
	"custom": {
		"modmenu": {
			"links": {
				"modmenu.discord": "${discord}",
				"modmenu.modrinth": "${modrinth}",
				"modmenu.curseforge": "${curseforge}"
			},
			"badges": [ "library" ],
			"update_checker": true
		}
	}
}
